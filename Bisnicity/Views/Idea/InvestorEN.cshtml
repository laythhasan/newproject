@model Bisnicity.Models.InvistorViewModel

@{
    var mess = TempData["messsages"] ?? string.Empty;
    var mess2 = TempData["messsageserro"] ?? string.Empty;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="~/css/login.css">
    <link href="~/css/toastr.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
    <section>
        <div class="container h-100" id="container h-1000" style="margin-top: 7rem;">
            <div class="row d-flex justify-content-center align-items-center h-100" id="row d-flex justify-content-center align-items-center h-1000">
                <div class="col col-xl-10">
                    <div class="card" id="cardd">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="card">
                                    <div class="card-body" style="padding: 0 !important;">
                                        <img src="~/images/login/login.png" class="card" alt="..." width="100%">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-7">
                                <div>
                                    <a href="#" type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" id="btn-closee"></a>
                                </div>
                                <h2 style="text-align: center; margin-top: 10%;">Complet Profile Invistor</h2>
                                <div class="card-body p-4 p-lg-5 text-black">
                                    <form asp-action="InvestorEN" asp-controller="Idea" enctype="multipart/form-data" method="post">
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form2Example17" id="form-labell">Compan's Name</label>
                                            <input asp-for="CompanyName" type="text" id="form2Example17" class="form-control form-control-lg" style="background: #F0F0F0;" />
                                            <span asp-validation-for="CompanyName" class="text-danger"></span>

                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label" id="form-labell">TargetSector</label>
                                            <select asp-for="targetsecid" class="form-control"
                                                    asp-items="@(new SelectList(Model.Targetsectors,"TargetsectorID","namesector"))"></select>
                                            <span asp-validation-for="targetsecid" class="text-danger"></span>
                                        </div>
                                        <div class="pt-1 mb-4">
                                            <button class="btn btn-lg" type="submit" id="btnbtn">Submit</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

</body>
</html>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var ss = '@mess';
        if (ss) {
            toastr.success(ss);
        }
    })
</script>
<script type="text/javascript">
    $(document).ready(function () {
        var ss = '@mess2';
        if (ss) {
            toastr.error(ss);
        }
    })
</script>