@model RegiseterViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="~/css/login.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
    <section>
        <div class="container h-100" id="container h-1000" style="margin-top:3%">
            <div class="row d-flex justify-content-center align-items-center h-100" id="row d-flex justify-content-center align-items-center h-1000">
                <div class="col col-xl-10">
                    <div class="card" id="cardd">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="card">
                                    <div class="card-body" style="padding: 0 !important;">
                                        <img src="~/images/login/loginn.png" class="card" alt="..." width="100%">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-7">
                                <div>
                                    <a href=@Url.Action("Index1","Home") type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" id="btn-closee"></a>
                                </div>
                                <div class="card-body p-4 p-lg-5 text-black">
                                    <form class="row g-3" method="post" asp-controller="Account" asp-action="Register" enctype="multipart/form-data">
                                        @Html.HiddenFor(x => x.sRoles)
                                        <div asp-validation-summary="All" class="text-danger"></div>
                                        <div class="col-md-6">
                                            <div class="form-outline">
                                                <label class="form-label" for="form2Example27" id="form-labell">First Name</label>
                                                <input asp-for="sFirstname" type="text" id="form2Example27" class="form-control form-control-lg" style="background: #F0F0F0;" />
                                                <span asp-validation-for="sFirstname" class="text-danger"></span>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-outline">
                                                <label class="form-label" for="form2Example27" id="form-labell">Last Name</label>
                                                <input asp-for="sLastname" type="text" id="form2Example27" class="form-control form-control-lg" style="background: #F0F0F0;" />
                                                <span asp-validation-for="sLastname" class="text-danger"></span>

                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-outline">
                                                <label class="form-label" for="form2Example17" id="form-labell">Email</label>
                                                <input asp-for="sEmail" type="email" id="form2Example17" class="form-control form-control-lg" style="background: #F0F0F0;" />
                                                <span asp-validation-for="sEmail" class="text-danger"></span>

                                            </div>
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form2Example17" id="form-labell">Mobile Number</label>
                                            <input asp-for="sPhone" type="tel" id="form2Example17" class="form-control form-control-lg" style="background: #F0F0F0;" />
                                            <span asp-validation-for="sPhone" class="text-danger"></span>

                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form2Example27" id="form-labell">Password</label>
                                            <input asp-for="sPassword" type="password" id="form2Example27" class="form-control form-control-lg" style="background: #F0F0F0;" />
                                            <span asp-validation-for="sPassword" class="text-danger"></span>

                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form2Example27" id="form-labell">Confirm Password</label>
                                            <input asp-for="sConfirmPassword" type="password" id="form2Example27" class="form-control form-control-lg" style="background: #F0F0F0;" />
                                            <span asp-validation-for="sConfirmPassword" class="text-danger"></span>

                                        </div>
                                        <div class="col-md-6">
                                            <label class="label">Gender</label>
                                            <select asp-for="nGenderid" class="form-control" style="background-color: #F0F0F0;"
                                                    asp-items="@(new SelectList(Model.Genders, "GenderID", "GenderName"))">
                                            </select>
                                            <span asp-validation-for="nGenderid" class="text-danger"></span>
                                        </div>
                                        <div class="col-md-6">
                                            <label class="label">Roles</label>
                                            <select asp-for="sRoles" id="RolesValue" class="form-control" style="background-color: #F0F0F0;"
                                                    asp-items="@(new SelectList(Model.Roles, "Id", "Name"))">
                                            </select>
                                            <span asp-validation-for="sRoles" class="text-danger"></span>
                                        </div>
                                        <div class="pt-1 mb-4">
                                            <button class="btn btn-lg" type="submit" id="btnbtn">Sign up</button>
                                        </div>
                                        <p class="pa">
                                            have an account?
                                            <a href=@Url.Action("Login","Account") class="ab">Login</a>
                                        </p>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

</body>
</html>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script><script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#RolesValue").change(function () {
                var value = $("#RolesValue option:selected");
                $("#sRoles").val(value.text());
            });
        });
    </script>
